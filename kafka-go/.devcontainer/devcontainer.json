{
  "name": "Golang 1.23 & Kafka",
  "dockerComposeFile": "docker-compose.yml",
  "service": "sandbox",
  "workspaceFolder": "/workspace",
  "customizations": {
    "vscode": {
      "extensions": [
        "golang.go"
      ],
      "settings": {
        "editor.formatOnSave": true,
        "settings": {
          "terminal.integrated.shell.linux": "/bin/bash"
        }
      }
    }
  }
}